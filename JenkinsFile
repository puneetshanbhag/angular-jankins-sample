pipeline {
    agent any

    stages {
        stage('Checkout SCM') {
            steps {
                 git branch: 'main', url: 'https://github.com/puneetshanbhag/angular-jankins-sample.git'
            }
        }
        stage('Install Node Modules') {
            steps {
                 sh "npm install"
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}